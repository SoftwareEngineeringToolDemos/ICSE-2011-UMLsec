<web-app>

<servlet>
	<servlet-name>vikiweb</servlet-name>
    <display-name>vikiweb</display-name>
    <servlet-class>tum.umlsec.viki.framework.web.LoaderWeb</servlet-class>
    
	<init-param>
		<param-name>ResourceRoot</param-name>
		<param-value>C:/__umlsec/</param-value>
	</init-param>
    
    
    
<!--    
	<init-param>
		<param-name>useodbc</param-name>
		<param-value>false</param-value>
	</init-param>
-->	  
</servlet>
  
<servlet-mapping>
	<servlet-name>
		vikiweb
	</servlet-name>
	<url-pattern>
		/vikiweb
	</url-pattern>
</servlet-mapping>



