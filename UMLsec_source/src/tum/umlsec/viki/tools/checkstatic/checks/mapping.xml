<?xml version="1.0"?>
<!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd">
<mapping>
  <description>Collection mapping</description>
  <class name = "tum.umlsec.viki.tools.checkstatic.checks.AttackerBean">
    <map-to xml = "attackerbean"/>
    <field name = "name" type = "string">
      <bind-xml name = "name" node="attribute"/>
    </field>
    <field name = "beans" type = "Bean" collection = "collection"/>
  </class>
  <class name = "tum.umlsec.viki.tools.checkstatic.checks.Bean" auto-complete="true">
    <map-to xml = "bean"/>
    <field name = "stereotype" type = "string">
      <bind-xml name = "stereotype" node = "attribute"/>
    </field>
    <field name = "threat1" type = "string">
      <bind-xml name = "threat1"/>
    </field>
    <field name = "threat2" type = "string">
      <bind-xml name = "threat2"/>
    </field>
    <field name = "threat3" type = "string">
      <bind-xml name = "threat3"/>
    </field>
    <field name = "threat4" type = "string">
      <bind-xml name = "threat4"/>
    </field>
  </class>
</mapping>